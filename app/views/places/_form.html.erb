    <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
    </div>
    

    <div class="field">
    <%= f.label :lat %>
    <%= f.number_field :lat, id: 'lat' %>
    </div>

    <div class="field">
    <%= f.label :lng %>
    <%= f.number_field :lng, id: 'lng' %>
    </div>

