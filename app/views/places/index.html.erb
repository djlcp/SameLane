<h3 id="notice">Places</h3>
<div>

<% @places.each do |place| %>
  <p>
    <div>Location: <%= place.name %></div><br />
    <div>Lat: <%= place.lat %></div><br />
    <div>Lng: <%= place.lng %></div><br />
    <%= link_to 'Edit', edit_place_path(place) %>
    <%= link_to 'Delete', place, method: :delete, data: { confirm: 'Are you sure?' } %>
  </p>
<% end %>

<%= link_to "New place", new_place_path %><br />